{
  "Website": {
    "commands": ["git -C /home/joe/Website pull",
                "cp /home/joe/Website/*.html /var/www/html/",
		"cp /home/joe/Website/*.css /var/www/html/",
		"cp /home/joe/Website/*.js /var/www/html/"
		]
  },
  "GitHubAutoDeploy": {
    "commands": ["git -C /home/joe/GitHubAutoDeploy pull"
                ]
  },
  "GoogleGarbler": {
    "commands": ["git -C /home/joe/GoogleGarbler",
                 "make -C /home/joe/GoogleGarbler",
                 "cp /home/joe/GoogleGarbler/*.html /var/www/html/GoogleGarbler",
                 "cp -r /home/joe/GoogleGarbler/css /var/www/html/GoogleGarbler/css",
                 "cp -r /home/joe/GoogleGarbler/js /var/www/html/GoogleGarbler/js"
                ]
  }
  
}
