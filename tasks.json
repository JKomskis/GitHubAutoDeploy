{
  "Website": {
    "commands": ["git -C /home/joe/Website pull",
                "html-minifier --input-dir ./ --output-dir build --file-ext html",
                "crass solarized.css --optimize --O1 > build/solarized.css",
                "cp /home/joe/Website/build/* /var/www/html/"
		]
  },
  "GitHubAutoDeploy": {
    "commands": ["git -C /home/joe/GitHubAutoDeploy pull"
                ]
  },
  "GoogleGarbler": {
    "commands": ["git -C /home/joe/GoogleGarbler pull",
                 "make -C /home/joe/GoogleGarbler/",
                 "mkdir /home/joe/GoogleGarbler/build",
                 "mkdir -p -m 775 /home/joe/GoogleGarbler/css",
                 "mkdir -p -m 775 /home/joe/GoogleGarbler/js",
                 "mkdir -p -m 775 /var/www/html/GoogleGarbler",
                 "html-minifier --input-dir ./ --output-dir build --file-ext html",
                 "crass css/style.css --optimize --O1 > build/css/style.css",
                 "uglifyjs js/script.js --compress --mangle -o build/js/script.js",
                 "cp -r /home/joe/GoogleGarbler/build/* /var/www/html/GoogleGarbler/"
                ]
  }
  
}
